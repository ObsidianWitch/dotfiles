<?xml version="1.0" encoding="UTF-8"?>
<style-scheme id="desertkai" _name="Desertkai" version="1.0">

  <_description>Modified Desert theme.</_description>

  <!-- Global Settings -->
  <style name="text" foreground="#d3d7cf" background="#2e3436"/>
  <style name="selection" foreground="#eeeeec" background="#888a85"/>
  <style name="cursor" foreground="#d3d7cf"/>
  <style name="current-line" background="#555753"/>
  <style name="line-numbers" foreground="#888888" background="#202020"/>
  <style name="draw-spaces" foreground="#888888"/>
  <style name="background-pattern" background="#34393A"/>
  <style name="right-margin" foreground="#eeeeec" background="#bababa"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" foreground="#f92672" bold="true"/>
  <style name="bracket-mismatch" background="#d3d7cf" bold="true"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#888888"/>
  <style name="def:shebang" foreground="#888888" bold="true"/>
  <style name="def:doc-comment-element" italic="true"/>

  <!-- Constants -->
  <style name="def:constant" foreground="#FFA0A0"/>
  <style name="def:string" foreground="#FFA0A0"/>
  <style name="def:special-char" foreground="#FFA0A0"/>
  <style name="def:special-constant" foreground="#FFA0A0"/>
  <style name="def:floating-point" foreground="#FFA0A0"/>

  <!-- Identifiers -->
  <style name="def:identifier" foreground="#729fcf"/>

  <!-- Statements -->
  <style name="def:statement" foreground="#f0e68c" bold="true"/>

  <!-- Types -->
  <style name="def:type" foreground="#a6e22e"/>

  <!-- Others -->
  <style name="def:preprocessor" foreground="#f92672"/>
  <style name="def:error" foreground="#d3d7cf" background="#f92672" bold="true"/>
  <style name="def:underlined" italic="true" underline="single"/>

</style-scheme>
